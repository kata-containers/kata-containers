FROM quay.io/centos/centos:@OS_VERSION@
@SET_PROXY@

RUN dnf -y update && \
    dnf -y install dnf-plugins-core && \
    dnf config-manager --set-enabled crb && \
    dnf -y install \
    diffutils \
    file \
    g++ \
    git \
    protobuf-compiler

@INSTALL_RUST@
