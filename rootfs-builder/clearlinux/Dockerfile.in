From fedora:30

@SET_PROXY@

RUN dnf -y update && dnf install -y \
chrony \
curl \
gcc \
git \
libseccomp \
libseccomp-devel \
make \
pkgconfig \
systemd

# This will install the proper golang to build Kata components
@INSTALL_GO@
