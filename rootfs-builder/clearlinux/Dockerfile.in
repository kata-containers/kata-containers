From fedora:27

@SET_PROXY@

RUN dnf -y update && dnf install -y \
git \
systemd \
pkgconfig \
gcc \
coreutils \
libseccomp \
libseccomp-devel \
chrony \
curl

# This will install the proper golang to build Kata components
@INSTALL_GO@
